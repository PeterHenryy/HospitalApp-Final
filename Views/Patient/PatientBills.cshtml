@model List<Bill>

<table class="table table-striped table-dark">
    <tr>
        <th>ID</th>
        <th>UserID</th>
        <th>User Name</th>
        <th>Bill Total</th>
        <th>Insurance</th>
        <th>Action</th>
    </tr>

    @foreach(var bill in Model)
    {
        <tr>
            <td>@bill.Id</td>
            <td>@bill.Appointment.UserID</td>
            <td>@bill.Appointment.User.UserName </td>
            <td>@bill.Total </td>
            <td>@bill.InsuranceId </td>
            <td> <a class="btn btn-primary" href="@Url.Action("AppointmentDetails", "Patient", new {appointmentID = bill.AppointmentId})">Details</a></td>
        </tr>
    }

</table>